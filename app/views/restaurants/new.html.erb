<div class='container'>
  <h2> Add a new Restaurant</h2>
  <%= simple_form_for @restaurant do |f| %>
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>  <%= f.input :name %>
    <%= f.input :category, collection: ["Chinese", "Italian", "Japanese", "French", "Belgian"] %>
    <%= f.input :address %>
    <%= f.input :phone_number %>
    <%= f.submit class: 'btn btn-primary' %>
    <% end %>
</div>
